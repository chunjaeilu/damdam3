    <script>
      setTimeout(() => {
        let link = "/main";
        let link2 = "/NoMoreInfo";
        let link3 = "/UserName";
        let userName = "<%= userArr[0].userName %>";
        let StartYear = "<%= userArr[0].StartYear %>";
        if (userName.length == 0) {
          location.href = link3;
        } else if (StartYear.length == 0 && userName.length >= 1) {
          location.href = link2;
        } else {
          location.href = link;
        }
      }, 1000);
    </script>
